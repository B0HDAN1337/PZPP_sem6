@{
    ViewData["Title"] = "Groups";
    var htmlContent = ViewData["HtmlContent"] as string;
}


<div class="row">
    <div class="col-md-4">
        <h2>Load the plan</h2>
        <form asp-controller="Group" asp-action="GroupScrap" method="post">
            <button type="submit">Load</button>
        </form>
    </div>
    <table class="table">
        <thead>
            <tr>
                <th>Group</th>
            </tr>
        </thead>
        <tbody>
            @Html.Raw(htmlContent)
        </tbody>
    </table>
</div>






